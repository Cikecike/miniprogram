/* pages/BrowseGoods/BrowseGoods.wxss */
page{
  background-color:#f5f5f5;
}

.Browser{
  display: flex;
  flex-direction: column;
  /* background-color: #f5f5f5; */
  width: 100%;

}

.follow_icon{
  position: fixed;
  margin: 0 10rpx;
  width:70rpx;
  height:70rpx;
  bottom: 30rpx;
  right: 20rpx;
}

.headers{
  height: 80rpx;
  margin: 0 0 50rpx 0 0;
  display: flex;
  justify-content: center;
  align-content: center;
  background-color: #c3ccdb;
  font-size: 35rpx;
  font-weight: bold;
  color: white;
}

.None_info{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 1000rpx;
  width: 100%;
}

.tips{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 50rpx;
  font-weight: bolder;
  color: rgba(192, 187, 187, 0.61);
}

